<launch>
    <node pkg="swarm_planner" type="usv_planner" name="usv_planner_1">
        <param name="usv_id" value="1"/>
    </node>
    <node pkg="swarm_planner" type="usv_planner" name="usv_planner_2">
        <param name="usv_id" value="2"/>
    </node>
    <node pkg="swarm_planner" type="usv_planner" name="usv_planner_3">
        <param name="usv_id" value="3"/>
    </node>
    <node pkg="swarm_planner" type="usv_planner" name="usv_planner_4">
        <param name="usv_id" value="4"/>
